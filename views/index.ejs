<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1 id='home-page-title'>Welcome!</h1>
    <p>Welcome to <%= title %></p>
    <ul class="right">
      <li>
      <% if (user) { %>
        <a href="/auth/logout">Log Out</a>
      <% } else { %>
        <a href="/auth/google">Login with Google</a>
      <% } %>
      </li>
    </ul>
    <ul>Registered Users:
      <% users.forEach(u => { %>
        <li><%= u.name %></li>
      <% }) %>
    </ul>
  </body>
</html>
